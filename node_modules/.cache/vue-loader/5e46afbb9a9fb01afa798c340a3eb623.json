 = true;\r\n      this.name = \"\";\r\n      this.email = \"\";\r\n      this.message = \"\";\r\n    },\r\n    closeModal() {\r\n      this.showModal = false;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Kontaktformular-Stile */\r\n.contact {\r\n  background: #f9f9f9;\r\n  border-radius: 10px;\r\n  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.1);\r\n  padding: 30px;\r\n  width: 100%;\r\n  max-width: 500px;\r\n  margin: 40px auto;\r\n  transition: transform 0.3s;\r\n}\r\n\r\n.contact:hover {\r\n  transform: scale(1.05);\r\n}\r\n\r\nlabel {\r\n  margin: 15px 0 5px;\r\n  display: block;\r\n  font-weight: bold;\r\n  color: #333;\r\n}\r\n\r\ninput,\r\ntextarea {\r\n  width: 100%;\r\n  padding: 12px;\r\n  margin-bottom: 15px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 5px;\r\n  transition: border 0.3s;\r\n  font-size: 16px;\r\n}\r\n\r\ninput:focus,\r\ntextarea:focus {\r\n  border: 1px solid #333;\r\n  outline: none;\r\n}\r\n\r\nbutton {\r\n  width: 100%;\r\n  padding: 12px;\r\n  background-color: #007bff;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: background 0.3s;\r\n  font-size: 16px;\r\n}\r\n\r\nbutton:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n/* Modal Styles */\r\n.modal {\r\n  display: block;\r\n  position: fixed;\r\n  z-index: 1;\r\n  left: 0;\r\n  top: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  overflow: auto;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  padding-top: 60px;\r\n}\r\n\r\n.modal-content {\r\n  background-color: #fff;\r\n  margin: 5% auto;\r\n}\r\n\r\n.close {\r\n  color: #aaa;\r\n  float: right;\r\n  font-size: 28px;\r\n  font-weight: bold;\r\n}\r\n\r\n.close:hover,\r\n.close:focus {\r\n  color: black;\r\n  text-decoration: none;\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"]}]}